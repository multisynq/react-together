{
  "name": "reacttogether",
  "description": "React Together",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm -f package-lock.json && rm -f bun.lockb",
    "lines_of_code": "cloc --exclude-dir=node_modules --not-match-f='.*\\.(svg|md|toml)' .",
    "lines_of_code_dist": "cloc --exclude-dir=node_modules --not-match-f='.*\\.(svg|md|toml)' dist"
  },
  "dependencies": {
    "@croquet/react": "^1.4.4",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "bootstrap": "^5.3.3",
    "bootstrap-5-breakpoint-react-hook": "^1.1.0",
    "primereact": "^10.6.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.0.1",
    "react-json-view-lite": "^1.3.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "cloc": "^2.0.0-cloc",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "sass": "^1.75.0",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
