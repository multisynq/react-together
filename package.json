{
  "name": "react-together-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "website",
    "playground"
  ],
  "scripts": {
    "playground": "./scripts/run.sh playground",
    "website": "./scripts/run.sh website",
    "build": "npm run build -ws --if-present",
